import std.c.stdio

fn main(): int {
    "100 = ".printf();
    mult.call<int, int, int>(10, 10).print();
    return 0;
}

fn <I1, I2, O> call(function: (I1, I2) -> O, i1: I1, i2: I2): O {
    return function.identity<I1, I2, O>()(i1, i2);
}

fn <I1, I2, O> identity(function: (I1, I2) -> O): (I1, I2) -> O {
    return function;
}

fn mult(i1: int, i2: int): int {
    return i1 * i2;
}

fn print(i: int) {
    printf("%d ", i);
}
