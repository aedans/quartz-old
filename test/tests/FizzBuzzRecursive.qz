fn main()
    fizzBuzz(100)

fn fizzBuzz(i: int) {
    if (i > 1)
        fizzBuzz(i - 1)

    if (i % 5 == 0 && i % 3 == 0)
        printf("FizzBuzz ")
    else if (i % 3 == 0)
        printf("Fizz ")
    else if (i % 5 == 0)
        printf("Buzz ")
    else
        printInt(i)
}

