import std.c.stdio

fn main(): int {
    let one: long = 1;
    let two: int = 2;
    let three: short = 3;
    let four: char = 4;
    let result = addFour(one, two, three, four) as int;
    printf("10 = %d", result);
    return 0;
}

fn addFour(one: char, two: short, three: int, four: long): long {
    return one + four + three + two;
}
