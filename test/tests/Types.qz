fn main() {
    val one: long = 1
    val two: int = 2
    val three: short = 3
    val four: char = 4
    val result = addFour(one, two, three, four)
}

fn addFour(one: char, two: short, three: int, four: long): double = add(add(one, four), add(three, two))

fn add(one: short, two: int): long = %one + two%
