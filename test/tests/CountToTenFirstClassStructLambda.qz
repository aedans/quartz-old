import std.c.stdio

struct CountInfo {
    tester: (int) -> int
    modifier: (int) -> int
    printer: (int) -> void
}

fn main(): int {
    10.countTo(CountInfo(
        i: int -> { return i > 0; },
        i -> { return i - 1; },
        { printf("%d ", p0); }
    ));
    return 0;
}

fn countTo(i: int, countInfo: CountInfo) {
    if countInfo.tester(i) then countInfo.modifier(i).countTo(countInfo);
    countInfo.printer(i);
}
