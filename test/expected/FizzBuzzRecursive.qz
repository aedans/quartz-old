import std.c.stdio

fn main(): int {
    100.fizzBuzz();
    return 0;
}

fn fizzBuzz(i: int) {
    if (i > 1)
        (i - 1).fizzBuzz();

    if (i % 5 == 0 && i % 3 == 0)
        printf("FizzBuzz ");
    else if (i % 3 == 0)
        printf("Fizz ");
    else if (i % 5 == 0)
        printf("Buzz ");
    else
        printInt(i);
}

fn printInt(i: int) {
    printf("%d ", i);
}
